{
   "compilerOptions": {
      "lib": [
        "es5",
        "es6",
        "dom",
        "webworker"
      ],
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
      //"strict": true
   },
   //"exclude": ["dist", "src"]
   "exclude": ["dist", "node_modules","src", "client"]
}